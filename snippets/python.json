{
    "optional union none": {
        "prefix": "oun",
        "body": "${1:type} | None = None",
        "description": "type hint an optional argument"
    },
    "arrange act assert": {
        "prefix": "aaa",
        "body": [
            "# Arrange",
            "${1:pass}",
            "# Act",
            "${2:pass}",
            "# Assert",
            "${3:pass}"
        ],
        "description": "Basic structure guide for a unit test"
    },
    "dash block": {
        "prefix": "dash",
        "body": [
            "logging.debug('------------------------------------')",
            "logging.debug(${1:...})}",
            "logging.debug('------------------------------------')"
        ],
        "description": "Logging debug message wrapped in hyphenated logging messages for easy recognition when reading output"
    }
}
